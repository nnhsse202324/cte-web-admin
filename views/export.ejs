<!-- include header -->
<%- include('include/_header', {page: "export"}) %>
<!-- /include header -->

<!-- Main Site -->
<main>
  <h1>Export Page</h1>

  <button onclick="downloadEmptyCSV()">Download Empty CSV</button>

  <script>
    function downloadEmptyCSV() {
      const csvContent = "data:text/csv;charset=utf-8,\n";

      const encodedUri = encodeURI(csvContent);
      const link = document.createElement("a");
      link.setAttribute("href", encodedUri);
      link.setAttribute("download", "empty_file.csv");
      document.body.appendChild(link);
      link.click();
      document.body.removeChild(link);
    }
  </script>
</main>
<!-- /Main Site -->

<!-- include footer -->
<%- include('include/_footer') %>
<!-- /include footer -->
